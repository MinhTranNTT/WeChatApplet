/* 水平间距 */
.protocol {
  width: 98%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin: 20rpx 10rpx 20rpx 10rpx;
}
.protocol .content {
  font-size: 22rpx;
  margin-left: 4rpx;
}
.protocol .empty-area {
  height: 30rpx;
}
.protocol .title-1 {
  font-size: 24rpx;
  font-weight: 550;
}
.protocol .title-2 {
  font-size: 23rpx;
}
.protocol .special {
  display: flex;
  justify-content: space-between;
}
.protocol .special #agree {
  color: #FCF90C;
}

